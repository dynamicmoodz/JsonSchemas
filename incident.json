{
	"definitions": {
		"incident": {
 			"id": "https://raw.githubusercontent.com/dynamicmoodz/JsonSchemas/master/incident.json",
			"type": "object",
			"properties": {
				"url": {
					"type": "string"
				},
				"atlasExpiryDate": {
					"type": "string",
					"format": "date-time"
				},
				"date": {
					"type": "string"
				},
				"description": {
					"type": "string"
				},
				"address": {
					"$ref": "address.json#/definitions/address"
				},
				"offenses": {
					"type": "array",
					"items": {
						"$ref": "offense.json#/definitions/offense"
					}
				}
			},
			"required": ["address", "atlasExpiryDate"]
		}
	}
}