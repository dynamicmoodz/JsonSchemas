{
	"definitions": {
		"name": {
 			"id": "https://raw.githubusercontent.com/dynamicmoodz/JsonSchemas/master/name.json",
			"type": "object",
			"properties": {
				"url": {
					"type": "string"
				},
				"firstName": {
					"type": "string"
				},
				"middleName": {
					"type": "string"
				},
				"lastName": {
					"type": "string"
				},
				"fullName": {
					"type": "string"
				},
				"suffix": {
					"type": "string"
				},
				"image": {
					"type": "string"
				},
				"address": {
					"$ref": "address.json#/definitions/address"
				}
			},
			"required": ["firstName", "lastName"]
		}
	}
}